{
  "name": "@swapkit/api-sdk-typescript",
  "version": "0.1.0",
  "description": "Type-safe TypeScript client for the SwapKit API",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "bun": "./src/index.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/types/index.d.ts"
    }
  },
  "files": [
    "dist/",
    "src/"
  ],
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "openapi-fetch": "0.13.5"
  },
  "devDependencies": {
    "typescript": "5.7.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/swapkit/api-sdk-typescript.git"
  },
  "bugs": {
    "url": "https://github.com/swapkit/api-sdk-typescript/issues"
  },
  "homepage": "https://github.com/swapkit/api-sdk-typescript#readme",
  "keywords": [
    "swapkit",
    "swap",
    "dex",
    "aggregator",
    "thorchain",
    "chainflip",
    "cross-chain",
    "typescript",
    "sdk",
    "openapi"
  ]
}
