{
  "name": "@swapkit/api-sdk-typescript",
  "version": "1.0.0",
  "description": "Type-safe TypeScript SDK for the SwapKit API",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist/",
    "src/"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts"
  },
  "dependencies": {},
  "devDependencies": {
    "tsup": "^8.0.2",
    "typescript": "5.7.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/swapkit/api-sdk-typescript.git"
  },
  "bugs": {
    "url": "https://github.com/swapkit/api-sdk-typescript/issues"
  },
  "homepage": "https://github.com/swapkit/api-sdk-typescript#readme",
  "keywords": [
    "swapkit",
    "swap",
    "dex",
    "aggregator",
    "thorchain",
    "chainflip",
    "cross-chain",
    "typescript",
    "sdk",
    "openapi"
  ]
}
